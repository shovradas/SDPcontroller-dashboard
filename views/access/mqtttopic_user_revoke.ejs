<div class="my-5">
    <div class="card mb-2">
        <div class="card-body py-3">
            <div class="detail-label">ACCESS ID</div>
            <div class="detail-separator">:</div>
            <div class="detail-value"><%=obj.id%></div>
        </div>
    </div>
    <div class="card mb-2">
        <div class="card-body py-3">
            <div class="detail-label">TOPIC</div>
            <div class="detail-separator">:</div>
            <div class="detail-value"><a href="/mqtttopic/<%=obj.topic_id%>"><%=obj.topic_id%> - <%= obj.topic_name %></a></div>
        </div>
    </div>
    <div class="card">
        <div class="card-body py-3">
            <div class="detail-label">CLIENT</div>
            <div class="detail-separator">:</div>
            <div class="detail-value"><a href="/user/<%=obj.user_id%>"><%=obj.user_id%> - <%=obj.last_name%>, <%=obj.first_name%></a></div>
        </div>
    </div>    
</div>

<div class="text-danger mb-3">Are you sure want to revoke this access?</div> 

<form method="post">   
    <div class="mb-5">
        <button class="d-none d-sm-inline-block btn btn-sm btn-danger shadow-sm">
            <i class="fas fa-save fa-sm text-white-50"></i> Confirm
        </button>
        <a href="<%=moduleRoute%>/mqtttopic/user#headingAccess<%=obj.topic_id%>" class="d-none d-sm-inline-block btn btn-sm btn-dark shadow-sm">
            <i class="fas fa-plus fa-sm text-white-50"></i> Abort
        </a>
    </div>
</form>

<%- contentFor('button_panel') %>
<div class="text-right">
    <a href="<%=moduleRoute%>/mqtttopic/user#headingAccess<%=obj.topic_id%>" class="d-none d-sm-inline-block btn btn-sm btn-dark shadow-sm">
        <i class="fas fa-arrow-left fa-sm text-white-50"></i> Back to List
    </a>    
</div>