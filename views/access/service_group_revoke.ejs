<div class="my-5">
    <div class="card mb-2">
        <div class="card-body py-3">
            <div class="detail-label">ACCESS ID</div>
            <div class="detail-separator">:</div>
            <div class="detail-value"><%=obj.id%></div>
        </div>
    </div>
    <div class="card mb-2">
        <div class="card-body py-3">
            <div class="detail-label">SERVICE</div>
            <div class="detail-separator">:</div>
            <div class="detail-value"><a href="/service/<%=obj.service_id%>"><%=obj.service_id%> - <%= obj.service_name %></a></div>            
        </div>
    </div>
    <div class="card">
        <div class="card-body py-3">
            <div class="detail-label">CLIENT</div>
            <div class="detail-separator">:</div>
            <div class="detail-value"><a href="/group/<%=obj.group_id%>"><%=obj.group_id%> - <%=obj.group_name%></a></div>
        </div>
    </div>    
</div>

<div class="text-danger mb-3">Are you sure want to revoke this access?</div> 

<form method="post">   
    <div class="mb-5">
        <button class="d-none d-sm-inline-block btn btn-sm btn-danger shadow-sm">
            <i class="fas fa-save fa-sm text-white-50"></i> Confirm
        </button>
        <a href="<%=moduleRoute%>/service/group#headingAccess<%=obj.service_id%>" class="d-none d-sm-inline-block btn btn-sm btn-dark shadow-sm">
            <i class="fas fa-plus fa-sm text-white-50"></i> Abort
        </a>
    </div>
</form>

<%- contentFor('button_panel') %>
<div class="text-right">
    <a href="<%=moduleRoute%>/service/group#headingAccess<%=obj.service_id%>" class="d-none d-sm-inline-block btn btn-sm btn-dark shadow-sm">
        <i class="fas fa-arrow-left fa-sm text-white-50"></i> Back to List
    </a>    
</div>